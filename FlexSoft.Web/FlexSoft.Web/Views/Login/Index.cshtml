@using FlexSoft.Web.Models
@model LoginInfo

@{
    ViewBag.Title = "View";
}

<h2>View</h2>

@using (Html.BeginForm("Login", "Login"))
{
    @Html.LabelFor(x => x.Username);
    @Html.TextBoxFor(x => x.Username);
    @Html.LabelFor(x => x.Password);
    @Html.TextBoxFor(x => x.Password);
    <input type="submit" value="Submit" />
}